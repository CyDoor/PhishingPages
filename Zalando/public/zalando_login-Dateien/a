for (var key in translations) {
var parts = key.split('.');
var ref = translations;
for (var i = 0; i < parts.length; i += 1) {
if (typeof ref[parts[i]] === 'string') {
break;
} else if (typeof ref[parts[i]] !== 'object') {
ref[parts[i]] = {};
}
if (i === parts.length - 1) {
ref[parts[i]] = translations[key];
} else {
ref = ref[parts[i]];
}
}
if (parts.length > 1) {
translations[parts[0]][parts.slice(1).join('.')] = translations[key];
}
}
return translations;
})();
;
ZAL.staticImagePath = "https://secure-skin.ztat.net/s/phk/zalando/img";
ZAL.cdnPath = ZAL.staticImagePath.replace(/img$/,"");
ZAL.modalLoginEnabled = true;

ZAL.registrationNewLayoutEnabled = false;

ZAL.tracking = ZAL.tracking || {};
ZAL.trackingConfig = {
ajaxCart : {
useWebtrekkEcommerce : true,
gtm : true
}
};