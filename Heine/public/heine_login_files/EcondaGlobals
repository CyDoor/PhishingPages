	var current_custgroup=getECookie("custgroup");
	function emos_setcustgroup(custgroup,mode)
	{
	    setECookie("custgroup",custgroup);
	    if (typeof(jsPk)!="undefined" && custgroup!=2)
	    {
	       custgroup+=jsPk;
	    }
	    emospro.cgroup=custgroup;
	    if (mode=="dynamic") window.emosPropertiesEvent(emospro);
	    econdaConsole("CGroup set with cgroup= " + emospro.cgroup);
	    gaCustgroup=String(custgroup);
		if (typeof(_gaq)==='undefined')
		{
	    	_gaq = _gaq || [];
	    }
	    if(custgroup!=2)
		{
		    _gaq.push(['_setCustomVar', 1, 'GA_Kundengruppe', gaCustgroup, 1]); 

		}
		    _gaq.push(['_setCustomVar', 2, 'Econda_Kundengruppe', gaCustgroup, 2]);
	
	}
	
	if (currentUserType=='G' && current_custgroup!=1)
	{
		
	    if(e_template=="checkout.content.step3")
	    {
	        emos_setcustgroup(1,"static")
	    }
	}
	else if (currentUserType=='R' && current_custgroup!=0)
	{
	        emos_setcustgroup(0,"static")
	}
	if (current_custgroup==null)
	{
	    emos_setcustgroup(2,"static")
	}
	
	
