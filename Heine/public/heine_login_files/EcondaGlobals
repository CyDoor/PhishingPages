var ecRT=0;

function econdaConsole(dText)
{

    if (typeof(console) == 'object' && emosDebug==true) 
    {
        if (typeof (console.info) != 'undefined') 
        {
            
            if (typeof dText == 'object' && ecRT<2) 
            {
                ecRT++;
                console.info('Econda Debugging (Object)');
                for (var d in dText) 
                {
                    if (typeof dText[d] == 'object') 
                    {   
                        econdaConsole(dText[d]);
                        
                    } 
                    else 
                    {
                        console.info(d+' = ',dText[d]);
                        counter=0;
                    }
                }
            } 
            else 
            {
                console.info('Econda Debugging: %o', dText);
            }
        }
    }
}

function econdaFilterSet()
{
	
	var query = window.location.search.substring(1);
	if(query.indexOf('facet')!==-1)
	{
		emospro.sfilter=[];
	    var vars = query.split('&');
	    for (var i=0;i<vars.length;i++) 
	    {
	    	
	       	var pair = vars[i].split('=');
	        if(pair[0] === 'facet')
	        {
	        	var facet=decodeURIComponent(decodeURIComponent(pair[1])).replace(/"/g,'');
	        	
	        	facet=facet.replace('suchfarbe','Farbe')
	        	.replace('size_eu','Größe')
	        	.replace('size_uk','Größe')
	        	.replace('manufacturer','Marke')
	        	.replace('price_heinede','Preis')
	        	.replace('price_heineat','Preis')
	        	.replace('price_heinech','Preis')
	        	.replace('price_heinenl','Preis')
	        	.replace('bv_averageoverallrating_de','Bewertung')
	        	.replace('bv_averageoverallrating_fr','Bewertung')
	        	.replace('bv_averageoverallrating_nl','Bewertung')
	        	.replace('form','Form')
	        	.replace('std_design','Muster')
	        	.replace('figurtyp','Figurtyp')
	        	.replace('std_cupsize','Körbchengröße')
	        	.replace('absatzhoehe_metric','Absatzhöhe')
	        	.replace('height_metric','Länge')
	        	.replace('width_metric','Breite')
	        	.replace('depth_metric','Tiefe')
	        	.replace('halterung','Halterung')
	        	.replace('funktion','Funktion')
	        	.replace('waregroup','Kategorie');
	        	emospro.sfilter.push(facet.split(':'));
	        }
	    }
    }
}


function e_price_format(price)
{
	price=String(price);
	if (price!=0)
	{
	    if (price!="NULL")
	    {
	    	fprice=price.match(/[0-9]+[,.]{1}[0-9]{1,2}/);
	    	if (fprice!=null)
	    	{
	    		price=String(fprice);
	    	}
	    	price=price.replace(',','.');
	       	var price_array=price.split('.')
	       	if (price_array.length > 2)
	       	{
	            price=price_array[0]+price_array[1]+'.'+price_array[2];
	       	}
	       	if (price.match(/[.]/)==null)
	       	{
	       		price+='.00';
	       	}
	    }
    }
    return price;
}


function eCurrencyConvert(price)
{
	switch(e_domain)
	{
		case '3'://ch
		var emosCurrFactor=1.63934;
		break;
		case '5'://uk
		var emosCurrFactor=0.83;
		break;
		default:
		return price;	
	}
	if (typeof(emosCurrFactor)==='number')
	{
		price=parseFloat(e_price_format(price));
		price/=emosCurrFactor;
		price=price.toFixed(2);
	}
	return price;
}



function getECookie(name)
{
    name += "=";
    var len = name.length;

    var cookiestr = document.cookie;
    var namepos = cookiestr.indexOf(name);
    var returnvalue=null;
    if (namepos>-1)
    {
        returnvalue=cookiestr.substr(namepos+name.length);
        if (returnvalue.indexOf(";")>0)
        {
            returnvalue=returnvalue.substring(0,returnvalue.indexOf(";"));
        }
    }
    return returnvalue;
}


function setECookie(name, value, expires, path)
{
    var exp = new Date();
    if (typeof(value)=='undefined' || (typeof(value)!='number' && value==''))
    {
        expires=-1;
        value='-1';
    }
    var new_cookie = name + "=" + value;
    if (typeof(expires)!="undefined" && expires!=null)
    {
        if (expires>0)
        {
            exp.setTime(exp.getTime() + expires);
        }
        else
        {
            exp.setTime(exp.getTime() - expires);
        }
        new_cookie+= "; expires=" + exp.toUTCString();
    }
    if (path!=null)
    {
        new_cookie+="; path=" + path+";";
    }
    else
    {
        new_cookie+="; path=/;";
    }
    document.cookie=new_cookie;
    econdaConsole(new_cookie);
}

function eChangeOrder(original_sku, original_var, original_size, original_quantity, new_var, new_size, new_quantity, ev_name, originalPrice,
						e_marker, newArtno, ev_category, ev_source, newAvail, ev_bundle, ev_mkz, newSKU, ev_bdf, e_sid, e_wkid, newPrice, oldAvail)
{
	if (typeof emospro !== 'undefined' && typeof emospro.basket !== 'undefined') {
		delete emospro.basket;
	}
	
	if (typeof emospro !== 'undefined' && typeof emospro.orderProcess !== 'undefined') {
		delete emospro.orderProcess;
	}
	
    ev_source="ChangeBest";
    var splitted_sku=original_sku.split("-");
    ev_artno=splitted_sku[0];
    ev_sku=splitted_sku[0] + "-" + splitted_sku[1];
    ev_sku=ev_sku.split("@")[0];

    if (emosDebug==true)
    {
        var d_Output="Original SKU: "+ original_sku +"\n";
        d_Output+="Original Var: "+ original_var +"\n";
        d_Output+="Original Size: "+original_size+"\n";
        d_Output+="Original Amount: "+original_quantity+"\n";
        d_Output+="xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx\n";
        d_Output+="New Var:"+new_var+"\n";
        d_Output+="New Size:"+new_size+"\n";
        d_Output+="New Amount: "+new_quantity+"\n";
    }
    if (original_var==new_var && original_size==new_size && original_quantity == new_quantity)
    {
        if (emosDebug==true)
        d_Output+="Nothing was changed!\n";
    }
    else
    {
        if (emosDebug==true)
        d_Output+="Article will be removed completely!\n";
        ev_amount=parseInt(original_quantity);
        if(getECookie("e_promo")!=null)
		{
    		ev_promo=getECookie("e_promo");
    	}
    	else
    	{
    		ev_promo='NULL';
    	}
        
        
        emospro.ec_Event = [];
        emospro.ec_Event.push(['c_rmv', ev_artno, ev_name, originalPrice, ev_category, original_quantity, ev_source, oldAvail, ev_bundle, ev_mkz, ev_sku, ev_bdf, ev_promo, original_size]);
        emospro.ec_Event.push(['c_add', newArtno, ev_name, newPrice, ev_category, new_quantity, ev_source, newAvail, ev_bundle, ev_mkz, newSKU, ev_bdf, ev_promo, new_size]);
        var changeLabel=new String();
        if (emospro.orderProcess==='1_Basket')
        {
        	changeLabel='Payment/Basket/ChangeOrderOL';
        }
        else if (emospro.orderProcess==='3_Basket')
        {
        	changeLabel='Payment/OrderExpress/ChangeOrderOL';
        }
        
        econdaOnClickEvent("c_rmv", ev_artno, ev_name, ev_price, "unknown" ,ev_amount, "unknown", ev_avail, ev_bundle, ev_style, ev_sku, ev_mkz, ev_lw, emospro.heinesid, emospro.heinewkid, e_marker, changeLabel, true);
    }
    econdaConsole(d_Output);
}

function econdaOnClick(content, url, e_sid)
{

    var emospro = {};
    if (e_domain=="Heine-HeineNL-Site")
    {
       content=content.replace("Startseite","Home");
    }
    if (e_domain=="Heine-HeineCh-Site" && e_language=="fr")
    {
       content=content.replace("Startseite","Accueil");
    }
    if (typeof(e_sid)!="undefined")
    {
       emospro.heinesid=e_sid;
    }
    else
    {
        if (getECookie("JSESSIONID")!=null)
       	{
			emospro.heinesid=getECookie("JSESSIONID");
	    }	
		else
		{
			emospro.heinesid="NULL";
		}
    }
    if (content.indexOf("Fillview")>-1)
    {
        emospro.marker="Fillview";
    }
    if (content.indexOf("WKAbbruch")>-1)
    {
        emospro.marker="WKAbbruchLayer";
    }
    if (content.indexOf("VIL")>-1)
    {
            emospro.marker="VIL";
    }
    if (content.indexOf("Home")>-1 && e_domain!="Heine-HeineNL-Site")
    {
        content=content.replace("Home","Startseite");
    }
    if (typeof(content)!="undefined" && typeof(url)!="undefined")
    {
        if (content!="empty")
        {
            emospro.content=content;
        }
        if (url!="empty")
        {
            emospro.url=url;
        }

        econdaConsole("OnClick ContentLabel gesetzt: \nContent: "+content+"\nURL :"+url);
        econdaConsole("Heine SID: "+emospro.heinesid);

        window.emosPropertiesEvent(emospro);
    }
    if (typeof(emospro.marker)!="undefined")
    {
            econdaConsole("Marker gesetzt: "+ emospro.marker);
    }
    
    if (typeof(_gaq)!=="undefined")
    {
    	if(typeof(gaDebug)!=='undefined')
    	{
    		gado=new gaDebug();
       		gado.updateContent(emospro.content);
       	}
       	_gaq.push(['_trackPageview', emospro.content]);

    }
}


function copy_emospro(emospro)
{
    var emospro_copy={};
    
    for (value in emospro)
    {
        if (value!="marker" && value!="" && value!='search')
        {
            emospro_copy[value]=emospro[value];
        }

    }
    return emospro_copy;
}


function econdaOnClickEvent(type,ev_artno,ev_name,ev_price,ev_category,ev_amount,ev_source,ev_avail,ev_bundle,ev_mkz,ev_sku,ev_bdf,ev_size,e_sid,e_wkid,e_marker,e_content, useExistingEvent)
{
    
    if (ev_category.indexOf("Detailview")>-1)
    {
        ev_category=ev_category.replace("/Detailview","");
        ev_category=ev_category.replace("/Bundle-Detailview","");
    }
    ev_amount=parseInt(ev_amount);
    if (typeof(jsProductBDF)!="undefined")
    {
        ev_bdf=jsProductBDF;
    }
    
    if (typeof(e_content)!="undefined" && e_content!="")
    {
        emospro.content=e_content;
    }
    
    if (marker_set==true)
    {
        emospro=copy_emospro(emospro);
    }
    
    var i=0;
    while (i < arguments.length)
    {
        if (arguments[i]==null || arguments[i] =="" || typeof(arguments[i]) == "undefined" || arguments[i]=="NULL")
        {
            if (type=="view" || type=="c_add")
            {
                arguments[i]="NULL";
            }
            else
            {
                arguments[i]="unknown";
            }
        }
        i++;
    }
    
    if (type==="view") 
    {
        ev_amount=1;
    }
    
    if (emospro.content==="Startseite" || emospro.content==="Home" || emospro.content==="Accueil")
    {
        ev_source="HomepageOutfit";
    }
    
    if (ev_lw==="NULL" && ev_source==="NULL" && typeof(emospro.search)!=="string")
    {
        ev_source="Navigation";
    }
    if (typeof(eCurrencyConvert)==='function')
    {
    	ev_price=eCurrencyConvert(ev_price);
    }
    
    if (typeof(emospro.search!="undefined") && emospro.search!=null)
    {
        ev_source="Suche/"+emospro.search[0][2]+"/"+emospro.search[0][0];
    }
    if (ev_source.indexOf("Teaser")!=-1 && ev_source.indexOf("VAT")==-1)
    {
        ev_source="Teaser";
    }
	var ev_promo=getECookie('e_promo');
	if(ev_promo===null)
	{
		ev_promo='NULL';
	}
	if (!useExistingEvent) {
	    
	    emospro.ec_Event = [[type,ev_artno,ev_name,ev_price,ev_category,ev_amount,ev_source,ev_avail,ev_bundle,ev_mkz,ev_sku,ev_bdf,ev_promo,ev_size]];
	}
	
    var show_layers_cookie = getECookie("show_layers");
    if (show_layers_cookie==null) show_layers_cookie="false";
    switch (type)
    {
        case "view" :
        
        if(emospro.content=='Home/Detailview_iPadApp')
        {
            emospro.marker="iPadApp";
        }
        break;
        default:
        break;
    }
    if (emosDebug==true)
    {
        emosSmallLayer(type);
    }
    if (e_template.indexOf('checkout')===-1)
    {
		if (typeof(wtCountryLabel)==='string' && wtCountryLabel!=='default')
{
	var wtprodid=ev_sku.split('-')[0];
	try
	{
		if($('#longDesc').length)
		var description=$('#longDesc').text().trim()
		else
		var description=$('#auxDesc').text().trim()
		var productData=[wtCountryLabel+'-'+wtprodid,$('#js_colors .selected img').attr('alt'),ev_size,'',wtCountryLabel+'-'+ev_sku,description];
	}
	catch(e)
	{
		econdaConsole(e)
	}
	
	switch (type)
	{
	    case 'view':
	   	switch(String(ev_avail))
	    {
	        case '0':
	        var wtavail='Immediately-Available';
	        break;
	        case '1':
	        var wtavail='Not-Available';
	        break;
	    }
	    if (ev_avail.indexOf('2')!=-1)
	    {
	        var wtavail='Not-Immediately-Available';
	    }

	    dcsMultiTrack('WT.dl','99','WT.cg_n','ProductDetailPage','WT.tx_e','v','WT.tx_u',String(ev_amount),'WT.tx_sv',ev_price,'WT.pn',ev_name,'WT.z_productdata',productData.join(),'WT.z_productid',wtCountryLabel+'-'+wtprodid,'WT.pn_sku',wtCountryLabel+'-'+ev_sku,'WT.z_rating',wtGetRating(),'WT.z_available',wtavail,'WT.z_recserved','','WT.z_recposition','','WT.z_recproductid','','WT.oss','','WT.oss_r','')
	    break;
	    case 'c_add':

	    if (typeof(emospro.heinewkid)==="undefined")
	    {
	    	dcsMultiTrack('WT.dl','99','WT.cg_n','OverlaysandPopUps','WT.z_productid',wtCountryLabel+'-'+wtprodid,'WT.pn_sku',wtCountryLabel+'-'+ev_sku,'WT.tx_u',String(ev_amount),'WT.tx_sa',ev_price,'WT.tx_e','a','WT.z_recserved','','WT.z_recposition','','WT.z_recproductid','','WT.z_cartStart',1,'WT.z_productdata',productData.join(),'WT.oss','','WT.oss_r','')
	    }
	    else
	    {
	    	dcsMultiTrack('WT.dl','99','WT.cg_n','OverlaysandPopUps','WT.z_productid',wtCountryLabel+'-'+wtprodid,'WT.pn_sku',wtCountryLabel+'-'+ev_sku,'WT.tx_u',String(ev_amount),'WT.tx_sa',ev_price,'WT.tx_e','a','WT.z_recserved','','WT.z_recposition','','WT.z_recproductid','','WT.z_productdata',productData.join(),'WT.oss','','WT.oss_r','')
	    }
	    break;
	    case 'c_rmv':
	    dcsMultiTrack('WT.dl','99','WT.cg_n','OverlaysandPopUps','WT.z_productid',wtCountryLabel+'-'+wtprodid,'WT.pn_sku',wtCountryLabel+'-'+ev_sku,'WT.tx_u',String(ev_amount),'WT.tx_sr',ev_price,'WT.tx_e','r','WT.z_recserved','','WT.z_recposition','','WT.z_recproductid','','WT.z_productdata',productData.join())
	    break;
	    
	    
	}
}


    }
    if (typeof(e_sid)!="undefined")
    {
       emospro.heinesid=e_sid;
    }
    if (typeof(e_wkid)!="undefined")
    {
        emospro.heinewkid=e_wkid;
    }
    if (typeof(e_marker)!="undefined")
    {
        emospro.marker=e_marker;
    }
    
    window.emosPropertiesEvent(emospro);
    if (typeof(emos_clickIn)=='number')
	{
		emos_clickIn = 0;
	}
    
    marker_set=true;
     
    if (typeof(_gaq)!=="undefined")
    {

       	_gaq.push(['_trackPageview', emospro.content]);
       	if(typeof(gaDebug)!=='undefined')
    	{
    		gado=new gaDebug();
    		if($('#gaContent').length)
    		gado.updateContent(emospro.content);
    		else
       		gado.addLine('_trackPageview', emospro.content,$('#gaDL'));
       	}

    }
}



function eMultiEvent(type,eItems)
{
	emospro.ec_Event=[];
	for (eItem in eItems)
	{
	 	
	 	emospro.ec_Event.push(eItems[eItem]);
	 	if (typeof(dcsMultiTrack)==='function')
	 	{
	 		if(e_template==='checkout.content.step1')
	 		{
	 			if (typeof(emospro.heinewkid)==="undefined")
	    		{
	    			dcsMultiTrack('WT.dl','99','WT.cg_n','OnlineOrderForm','WT.z_productid',wtCountryLabel+'-'+eItems[eItem][1],'WT.pn_sku',wtCountryLabel+'-'+eItems[eItem][10],'WT.tx_u',eItems[eItem][5],'WT.tx_sa',eItems[eItem][3]*eItems[eItem][5],'WT.tx_e','a','WT.z_recserved','','WT.z_recposition','','WT.z_recproductid','','WT.z_cartStart',1,'WT.pn',eItems[eItem][2])
	    		}
	    		else
	    		{
	    			dcsMultiTrack('WT.dl','99','WT.cg_n','OnlineOrderForm','WT.z_productid',wtCountryLabel+'-'+eItems[eItem][1],'WT.pn_sku',wtCountryLabel+'-'+eItems[eItem][10],'WT.tx_u',eItems[eItem][5],'WT.tx_sa',eItems[eItem][3]*eItems[eItem][5],'WT.tx_e','a','WT.z_recserved','','WT.z_recposition','','WT.z_recproductid','','WT.pn',eItems[eItem][2])
	    		}
	    	}
	    }
	}
}


function econdaTarget(group, name, value, calc) {
     if(typeof(window.emospro) == 'undefined') {
          window.emospro = {};
     }
     window.emospro.Target = [[group, name, value, calc]];
     window.emosPropertiesEvent(window.emospro);
}

function fillSKUWithLeadingZeroes(sku) 
{
	if (null != sku) 
	{
		skuSplit = sku.split("-");
		while (skuSplit[0].length < 6) 
		{
			skuSplit[0] = "0" + skuSplit[0];
		}
		sku = skuSplit[0];
		for (i = 1; i < skuSplit.length; i++) 
		{
			sku += "-" + skuSplit[i];
		}
		return sku;
	} 
	else 
	{
		return "";
	}
}
function econdaSepia()
{
	if (sessionStorage && sessionStorage.getItem('sepia:data') !== null) 
	{
		if(sessionStorage.getItem('econdaSepia')===null)
		{
			var sObj=JSON.parse(sessionStorage.getItem('sepia:data'))[0];
			if(typeof(sObj.Grp)!=='undefined')
			{
				emospro.kdset=[[String(sObj.Grp)]];
			}
			if(typeof(sObj.KuKlaTOP)!=='undefined')
			{
				emospro.ratingset=[[String(sObj.KuKlaTOP)]];
			}
			var custinfo=[];
			if(typeof(sObj.Misch)!=='undefined')
			{
				custinfo.push(String(sObj.Misch));
			}
			else
			{
				custinfo.push('undefined');
			}
			var actTime=Date.now();
			if(typeof(sObj.ampelRot)!=='undefined' && sObj.ampelRot < actTime)
			{
				custinfo.push('rot');
			}
			else if(typeof(sObj.ampelGelb)!=='undefined' && sObj.ampelGelb < actTime)
			{
				custinfo.push('gelb');
			}
			else if(typeof(sObj.ampelValide)!=='undefined' && sObj.ampelValide)
			{
				custinfo.push('grün');
			}
			else
			{
					custinfo.push('undefined');
			}

			if(typeof(sObj.retourenstatus)!=='undefined')
			{
				custinfo.push(sObj.retourenstatus);
			}
			else
			{
				custinfo.push(9);
			}
			if(typeof(sObj.NL)!=='undefined')
			{
				custinfo.push(sObj.NL);
			}
			else
			{
				custinfo.push(9);
			}
			if(typeof(sObj.CustomerID)!=='undefined')
			{
				custinfo.push(String(sObj.CustomerID));
			}
			else
			{
				custinfo.push('undefined');
			}
			emospro.cust=custinfo;
			sessionStorage.setItem('econdaSepia',true);
			econdaConsole('sepia data sent to econda');
		}
	}
	else
	{
		econdaConsole('sepia data already sent to econda');
	}
}
emosDebug=false;

if(emosDebug && typeof window.history == "object" && typeof window.history.pushState == "function") {
	var newUrl = window.location.toString();
	if(newUrl.indexOf("emosdebug") == -1) {
		var connector = (newUrl.indexOf("?") > -1) ? "&" : "?";
		var emosDebugParam = "emosdebug=yxcvbnm";
		var urlSplit = newUrl.split("#");
		if(urlSplit.length > 1) {
			newUrl = urlSplit[0] + connector + emosDebugParam + "#" + urlSplit[1];
		} else {
			newUrl = urlSplit[0] + connector + emosDebugParam;
		}
		window.history.pushState("EcondaDebug", document.title, newUrl);
	}
}


emospro = {};

e_standardHome = "Startseite";
e_httpErrorResponseCode = "-1";
e_template="LogonSetup";


emos_searchQuery=false;
emos_searchCount=0;
emos_filters=false;
 
if (e_template=='searchResultList' || e_template=='noMatch' ||e_template=='productView' || e_template=='sh_bundle_detailview')
{
	var e_searchTerm = HashHandle().searchTerm || null;

	if ((typeof(emos_searchQuery)!='undefined' && emos_searchQuery!=false) || e_template==='noMatch' || (e_searchTerm !== null && $.trim(e_searchTerm) !== '')) {
        if (e_searchTerm !== null) {
        	emos_searchQuery = e_searchTerm;
        } else if (typeof(emos_searchQuery) === 'undefined' || !emos_searchQuery) {
            emos_searchQuery = '[leer]';
        }
	    var search_type='Suchbegriff';
	    if (emos_searchCount!=false)
	    {
			emos_searchCount=parseInt(emos_searchCount.replace('.',''));
		}
		
		else
		{
			if ($('#searchCount').length>0)
			emos_searchCount=$('#searchCount').text().replace('.','');
		}
		var re_printno=/[0-9]{8}/;
		if(document.location.hostname.indexOf('co.uk')==-1)
		{
			var re_artno=/[0-9]{6,7}[A-z]{0,1}/;
		}
		else
		{
			var re_artno=/[0-9]{6}[9]{0,1}/;
		}
		if (emos_searchQuery.length>=6 && emos_searchQuery.length<8)
		{
		    if (emos_searchQuery.match(re_artno)!=null)
		    {
		        search_type='Artikelnummer(online)';
		        if(e_template=='productView')
		        {
		        	emos_searchCount=1;
		        }
		    }
		}
		else if (emos_searchQuery.length==8)
		{	
			if(typeof(re_printno)=='object')
			{
			    if (emos_searchQuery.match(re_printno)!=null)
			    {
			        search_type='Artikelnummer(offline)';
			        if(e_template=='productView')
			        {
			        	emos_searchCount=1;
			        }
			    }
		    }
		}
		else
		{
		    setECookie('e_lS','/'+search_type+'/'+emos_searchQuery)
		}
		emospro.search = [[emos_searchQuery, emos_searchCount,search_type]];
	}
}
else if(window.document.URL.indexOf('vip-shopping')!=-1)
{
