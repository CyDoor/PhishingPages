	                        }
	                        try{
		                        _tag.WT.pn_sku=wtSKUs;
		                        _tag.WT.z_productid=wtPIDs
		                        _tag.WT.tx_u=wtQuants;
		                        _tag.WT.pn=wtPNames;
		                        _tag.WT.tx_s=wtPrices;
		                        switch(econdaOrder.orderShippingInfo.shippingMode.shippingModeIdentifier.externalIdentifier.shipModeCode)
		                        {
		                        	case 'DEFAULT':
		                        	_tag.WT.z_deliveryType='Invoice'
		                        	break;
		                        	case 'Flag_24h':
		                        	_tag.WT.z_deliveryType='Express'
		                        	break;
		                        	case 'Flag_DesiredDeliveryDate':
		                        	_tag.WT.z_deliveryType='Preferred Time of Delivery'
		                        	break;
		                        }
		                        if (econdaOrder.orderPaymentInfo.paymentInstruction[0].paymentMethod.paymentMethodName.indexOf('CREDIT_CARD'))
		                        {
		                        	_tag.WT.z_paymentType='Credit Card'
		                        }
		                        else if(econdaOrder.orderPaymentInfo.paymentInstruction[0].paymentMethod.paymentMethodName.indexOf('PURCHASE_ON_ACCOUNT'))
		                        {
		                        	_tag.WT.z_paymentType='Invoice'
		                        }
		                       	else if(econdaOrder.orderPaymentInfo.paymentInstruction[0].paymentMethod.paymentMethodName.indexOf('PAYPAL'))
		                        {
		                        	_tag.WT.z_paymentType='Paypal'
		                        }
		                        else if(econdaOrder.orderPaymentInfo.paymentInstruction[0].paymentMethod.paymentMethodName.indexOf('YAPITAL'))
		                        {
		                        	_tag.WT.z_paymentType='Yapital'
		                        }
		                     
		                     	_tag.WT.si_n='PurchaseScenario'
		                     	_tag.WT.si_x='3'
		                     	_tag.WT.si_cs='1'
		                        _tag.WT.z_shippingCost=String(econdaOrder.orderAmount.totalShippingCharge.value);
		                        _tag.WT.z_orderTotal=emospro.billing[0][3];
		                        _tag.WT.tx_i=emospro.billing[0][0];
		                        var wtTime=timeStamp().split('*');
		        
		                        _tag.WT.tx_id=wtTime[0].trim();
		                        _tag.WT.tx_it=wtTime[1].trim();
		  
	                        }
	                        catch(e)
	                        {
	                        	econdaConsole(e)
	                        }
	                        switch(e_domain)
	                        {
	                             default:_tag.WT.site='heine.de';break;
	                        }
	                    }
	                }
	                	                
	                	               
	               if ($('.recom-container').length)
	                	{
	                		var recomids=String();
	                		var recompos=String();
	                		var recnames=String();
	                		var reccount=1;
	                		$('.recom-container .recom-list-item').each(function(){
		                		if(reccount>1)
		                		{
		                			recomids+=';'
		                			recompos+=';'
		                			recnames+=';'
		                		}
							  	recomids+=wtCountryLabel+'-'+$(this).data('id')
							  	recompos+='rec-'+reccount
							  	recnames+=$(this).children('.recom-link').children('.a-recom-link').text()
							  	reccount++
							  	$(this).click(function(event){
							  		dcsMultiTrack('WT.dl', '99','WT.z_recclick','1','WT.z_recposition','rec-'+reccount,'WT.z_recproductid',wtCountryLabel+'-'+$(this).data('id'),'WT.z_recserved','','WT.tx_e','','WT.pn','','WT.pn_sku','','WT.z_productid','','WT.z_available','','WT.tx_sv','');
							  	})
							  
							})
							_tag.WT.z_recproductid=recomids
							_tag.WT.z_recposition=recompos
							if(wtContent!=='Homepage')
							_tag.WT.z_recserved=recnames	
	                	}
	
	            }
	            });
	            }
	            catch (e)
	            {
	               econdaConsole(e);
	            }
	        }
	        else
	        {
	            econdaConsole('Webtrends Object not ready yet. Tracking fails.');
	        }
	}


}


	    sendto_econda();
	}
	else
	{
	    econdaConsole("Konnte nicht an Econda schicken sendto_econda() ist nicht definiert");
	}
});


switch (e_domain)
{
    case "1":
		if(typeof(e_domain_type) === "string" && e_domain_type == "mobile" ){
			gaAccount="UA-30736482-3";
		}else{
			gaAccount="UA-30736482-1";
		}
    	gaDomain="heine.de";
    	break;
    case "2":
    	if (typeof(e_domain_type) === "string" && e_domain_type == "mobile" ) {
    		gaAccount="UA-30742039-4";
    	} else {
			gaAccount="UA-30742039-1";
		}
    	gaDomain="heine.at";
    	break;
    case "3":
    	if (typeof(e_domain_type) === "string" && e_domain_type == "mobile" ) {
    		gaAccount="UA-30746602-3";
    	} else {
    		gaAccount="UA-30746602-1";
    	}
    	gaDomain="heine.ch";
    	break;
    case "4":
    gaAccount="UA-30746414-1";
    gaDomain="heine-shop.nl";
    break;
    case "5":
    gaAccount="UA-47760394-1";
    gaDomain="heine.co.uk";
    break;
    
}
if (typeof(_gaq)==='undefined')
var _gaq = _gaq || [];
_gaq.push(['_setAccount', gaAccount]);                 
_gaq.push(['_gat._anonymizeIp']);                      
_gaq.push(['_setDomainName', gaDomain]);               
_gaq.push(['_setCampaignCookieTimeout', 2592000000]);  

if (typeof(gaAffiliateID)=='string')
_gaq.push(['_setCampSourceKey', gaAffiliateID]);   
if (typeof(gaActionID)=='string')
_gaq.push(['_setCampNameKey', gaActionID]);         