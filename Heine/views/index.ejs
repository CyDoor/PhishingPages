                e.preventDefault();
                //if searchterm is an articleNumber (first 6 chars are numeric) -> removes spaces in searchterm
                var searchTermStringWithoutSpaces = $('.searchTerm', searchContainer).val().replace(/ /g, '');
                if(searchTermStringWithoutSpaces == "") {
                    return false;
                }
                var artNumberTestString = searchTermStringWithoutSpaces.substr(0,6);
                if (artNumberTestString.length == 6) {
                    var isNumber = true;
                    var artNumberTestStringArray = artNumberTestString.split('');
                    $.each(artNumberTestStringArray, function(i) {
                        if (isNumber && !$.isNumeric(artNumberTestStringArray[i])) {
                            isNumber = false;
                        }
                    });
                    if (isNumber) {
                        $('.searchTerm', searchContainer).val(searchTermStringWithoutSpaces);
                    }
                }

                var searchTerm = $('.searchTerm', searchContainer).val();
                searchTerm = $.trim(searchTerm);
                window.location.href = searchBaseURL + encodeURIComponent(searchTerm);
